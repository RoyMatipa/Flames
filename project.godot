; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Flames"
run/main_scene="res://scenes/bootsplash.tscn"
boot_splash/image="res://assets/bootsplash.png"
boot_splash/bg_color=Color( 0.164706, 0.156863, 0.211765, 1 )
config/icon="res://assets/icons/project.png"
config/quit_on_go_back=false

[display]

window/size/width=1080
window/size/height=1920
window/size/test_width=1024
window/size/test_height=600
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_clear_color=Color( 0.164706, 0.156863, 0.211765, 1 )
environment/default_environment="res://default_env.tres"
